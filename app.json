{
  "expo": {
    "name": "AcousticOne",
    "slug": "AcousticOne",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lightAO.png",
    "scheme": "acousticone",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.acousticone.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lightAO.png",
        "backgroundColor": "#ffffff",
        "dark": {
          "foregroundImage": "./assets/images/darkAO.png"
        }
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/lightAO.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lightAO.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/darkAO.png",
            "backgroundColor": "#121212"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
